#include<stdio.h>
 void addArray(int a[],int b[]){
    int c[300];
    for(int i=0;i<10;i++){
     c[i]=a[i];
    }
    for(int i=0;i<10;i++){
     c[i+10]=b[i];
    }
    for(int i=0;i<20;i++){
    printf("%d  ",c[i]);
   }
}
void main(){
   int a[10];
   int b[10];
   printf("Enter the elements of first array :");
   for(int i=0;i<10;i++){
    scanf("%d",&a[i]);
   }
   printf("Enter the elements of second array :");
   for(int i=0;i<10;i++){
    scanf("%d",&b[i]);
   }
    addArrays(a,b);
}
